{
  "name": "wolox-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^11.0.4",
    "axios": "^0.20.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "^1.19.0",
    "enzyme-to-json": "^3.6.1",
    "install": "^0.13.0",
    "jest-svg-transformer": "^1.0.0",
    "lodash": "^4.17.20",
    "npm": "^6.14.8",
    "or": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-scroll": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "lint": "eslint server client --ignore-pattern client/src --ignore-pattern client/public --ignore-pattern legacy_notes_and_workbook || true",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest-transform-stub": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "\\.(css|less)$": "<rootDir>/__tests__/styleMock.js",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/fileMock.js",
      "<rootDir>/__tests__/styleMock.js",
      "<rootDir>/__tests__/jest.init.js"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/jest.init.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
